<!-- Slider section -->
    <section id="yg-slider">
      <div id="carouselYGHotel" class="carousel slide" data-ride="carousel">
         <ol class="carousel-indicators">
          {% set i = 0 %}
          {% for row in rows %}
              {% if i == 0 %}
                <li data-target="#carouselYGHotel" data-slide-to="{{i}}" class="active"></li>
              {% else %}
                <li data-target="#carouselYGHotel" data-slide-to="{{i}}"></li>
              {% endif %}
              {% set i = i + 1 %}
          {% endfor %}
         </ol>
         <div class="carousel-inner" role="listbox">
            {% for row in rows %}
              {{ row.content }}
            {% endfor %}
         </div>
      </div>
</section>
<!-- END BANNER SLIDER SECTION -->