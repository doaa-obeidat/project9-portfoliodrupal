<div class="col-md-4 col-sm-6 col-xs-12 blog-lists">
    <div class="blog-wrap mb-30">
        <div class="blog-img wow fadeInDown">
            <a href="{{fields.view_node.content}}">
                {{ fields.field_image.content}}
            </a>
        </div>
        <div class="blog-content wow fadeInUp">
            <div class="blog-meta">
              <span>{{fields.created.content}}</span>
            </div>
            <h3>
                <a href="{{fields.view_node.content}}">
                    {{fields.title.content}}
                </a>
            </h3>
            {{fields.body.content}}
        </div>
    </div>
</div>